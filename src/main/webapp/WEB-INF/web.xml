<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.ruhaim.appointment.controller.UserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>JobSeekerController</servlet-name>
  	<display-name>JobSeekerController</display-name>
  	<description></description>
  	<servlet-class>com.ruhaim.appointment.controller.JobSeekerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminController</servlet-name>
  	<display-name>AdminController</display-name>
  	<description></description>
  	<servlet-class>com.ruhaim.appointment.controller.AdminController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConsultantController</servlet-name>
  	<display-name>ConsultantController</display-name>
  	<description></description>
  	<servlet-class>com.ruhaim.appointment.controller.ConsultantController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AppointmentController</servlet-name>
  	<display-name>AppointmentController</display-name>
  	<description></description>
  	<servlet-class>com.ruhaim.appointment.controller.AppointmentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/UserManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>JobSeekerController</servlet-name>
  	<url-pattern>/JobSeekerManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminController</servlet-name>
  	<url-pattern>/AdminManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConsultantController</servlet-name>
  	<url-pattern>/ConsultantManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/AppointmentManager</url-pattern>
  </servlet-mapping>
</web-app>
